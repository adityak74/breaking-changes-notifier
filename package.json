{
  "name": "breaking-changes-notifier",
  "version": "1.0.0",
  "description": "An app to scrape and display breaking changes and versions for npm/git hosted packages.",
  "main": "src/server/index.js",
  "proxy": "https://api.saveur.app/api",
  "scripts": {
    "build": "webpack --mode production -p",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Aditya Karnam",
  "license": "ISC",
  "dependencies": {
    "@octokit/core": "3.5.1",
    "antd": "4.16.13",
    "babel-polyfill": "6.26.0",
    "cors": "2.8.5",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "git-url-parse": "11.5.0",
    "parse-github-url": "1.0.2",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-markdown": "7.0.1",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.1",
    "react-syntax-highlighter": "15.4.4",
    "recompose": "0.30.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "6.2.1",
    "css-loader": "6.2.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.25.1",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.2",
    "nodemon": "2.0.12",
    "style-loader": "3.2.1",
    "url-loader": "4.1.1",
    "webpack": "5.52.0",
    "webpack-cli": "4.8.0",
    "webpack-dev-server": "4.1.0"
  }
