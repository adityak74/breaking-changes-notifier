{
  "name": "breaking-changes-notifier",
  "version": "1.0.0",
  "description": "An app to scrape and display breaking changes and versions for npm/git hosted packages.",
  "main": "src/server/index.js",
  "proxy": "https://api.saveur.app/api",
  "scripts": {
    "build": "webpack --mode production -p",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Aditya Karnam",
  "license": "ISC",
  "dependencies": {
    "@octokit/core": "3.5.1",
    "antd": "4.13.0",
    "babel-polyfill": "6.26.0",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.16.3",
    "git-url-parse": "11.4.4",
    "parse-github-url": "1.0.2",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-markdown": "5.0.3",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.1",
    "react-syntax-highlighter": "15.4.3",
    "recompose": "0.30.0"
  },
  "devDependencies": {
    "@babel/core": "7.15.0",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-react": "7.14.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.2.2",
    "clean-webpack-plugin": "1.0.1",
    "concurrently": "4.1.2",
    "css-loader": "2.0.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.25.1",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.19.4",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.5.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.2"
  }
